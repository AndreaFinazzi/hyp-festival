<% include header %>

<!--cover section slider -->
<section id="home" class="home-cover">
    <div id="cover-slider-container" class="cover_slider owl-carousel owl-theme">
    </div>
    <div class="cover_nav">
        <ul id="cover-dots" class="cover_dots">
        </ul>
    </div>
</section>
<!--cover section slider end -->

<hr>

<!--about the event -->
<section class="pt100 pb100" data-module="main">
    <div class="container">
        <div class="section_title">
            <h3 id="content-title" class="title">
            </h3>
        </div>
        <div class="row justify-content-left">
            <div class="col-12 col-md-10">
                <p id="content-body justify text-last-end">
                </p>
            </div>
        </div>
    </div>
</section>

<hr>

<!--event calender-->
<section class="pt100 pb100">
    <div class="container">
        <div class="table-responsive">
            <table class="event_calender table">
                <thead class="event_title">
                    <tr>
                        <th>
                            <i class="ion-ios-pricetags-outline"></i>
                            <span>Next events</span>
                        </th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                    </tr>
                </thead>
                <tbody id="calendar-box-container">
                    <!-- user reservations here -->
                </tbody>
            </table>
        </div>
    </div>
</section>
<!--event calender end -->

<% include footer %>