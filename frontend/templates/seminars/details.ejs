<!--page title section-->
<section id="primary-photo" class="inner_cover parallax-window" data-parallax="scroll">
    <div class="overlay_dark"></div>
    <div class="container">
        <div class="row justify-content-center align-items-center">
            <div class="col-12">
                <div class="inner_cover_content">
                    <h3>
                        <%= data.title %>
                    </h3>
                </div>
            </div>
        </div>

        <div class="breadcrumbs">
            <ul>
                <li><a href="/">Home</a> | </li>
                <li><a href="/seminars">Seminars</a> | </li>
                <li><span><%= data.title %></span></li>
            </ul>
        </div>
    </div>
</section>
<!--page title section end-->

<section class="pb100 container">
    <div class="event_box">
        <div class="event_info">
            <div class="event_title">
                <%= data.title %>
            </div>
            <div class="speakers">
                <strong>Related Events</strong>
                <span><%= data.joinItems.map(item => { return '<a href="/events?id=' + item.id + '">' + item.title + ' </a>'}).join(', ') %></span>
            </div>
            <div class="event_date">
                <%= data.date %>
            </div>
        </div>
    </div>
    <div class="event_word">
        <div class="row justify-content-left" style="text-align: justify;">
            <div class="col-md-12 col-12">
                <%= data.fact_sheet %>
            </div>
        </div>
    </div>
</section>